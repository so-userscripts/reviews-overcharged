{
  "compilerOptions": {
    "strict": true,
    "target": "ES2019",
    "lib": ["ESNext", "DOM"],
    "outDir": "dist",
    "noImplicitReturns": true,
    "removeComments": true,
    "moduleResolution": "node",
    "skipDefaultLibCheck": true,
    "module": "none",
    "allowJs": true,
    "checkJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
